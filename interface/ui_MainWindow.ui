/*/////////////////////FRAMES//////////////////////////////*/
/*app_conteiner  */
#app_conteiner {	
	background-color: rgb(0, 117, 190);
	border: 1px solid rgb(0, 117, 190);
}
#tab_login{
	border: 3px solid rgb(39,129, 190);
	border-style: solid;
	border-radius: 5px;
}
#tab_register{
	border: 3px solid rgb(39,129, 190);
	border-style: solid;
	border-radius: 5px;
}
/*main_botton*/
#main_botton{
	background-color: rgb(255,255,255);
	border: 1px solid rgb(0, 117, 190);
	border-top: 3px solid rgb(0, 117, 190);
	
}
/*botton_bar*/
#botton_bar { 
	background-color: rgb(0, 117, 190); 
}

/*///////////////////// ELEMENTOS ESPECÍFICOS//////////////////////////////*/
/* botton_bar*/
#botton_bar QLabel { 
	font-size: 11px; color: rgb(113, 126, 149); 
	padding-right: 10px; 
	padding-left: 10px;
	padding-bottom: 2px; 
}

/* menu_box */
#menu_box .QPushButton {
    background-repeat: no-repeat;
    background-position: center center;	
	border-radius: 5px;
	border: none;
	background-color: transparent;
	text-align: left;
}
#menu_box .QPushButton:hover {
	background-color: rgb(162, 216, 230);
}
#menu_box .QPushButton:pressed {	
	background-color: rgb(39,129, 190);
	color: rgb(255, 255, 255);
}

#top_logo{
	border-top: 3px solid rgb(0, 117, 190);
}

/*main_top*/
#main_top .QPushButton { 
	background-color: rgba(255, 255, 255, 0);   
	border-radius: 5px;
    background-position: center;
    background-repeat: no-repeat;
	border: none;
	background-color: transparent;
	text-align: center;
}
#main_top .QPushButton:hover { 
	background-color: rgb(44, 49, 57); 
	border-style: solid; 
	border-radius: 4px; 
}
#main_top .QPushButton:pressed { 
	background-color: rgb(23, 26, 30); 
	border-style: solid; 
	border-radius: 4px; 
}

